<%- include partials/head %>

<h3 class="text-md-center">Your Profile</h3>
<div class="row">
  <div class="col-md-6 text-md-right">
    <img src="<%= uberProfile.picture %>"\>
  </div>
  <div class="col-md-6">
    <h4>
      <%= uberProfile.first_name %>&nbsp;<%= uberProfile.last_name %>
    </h4>
    <h5>
      <%= uberProfile.email %>
    </h5>
    <h6>
      <strong><label>Promocode: </label></strong> <%= uberProfile.promo_code %>
    </h6>
  </div>
</div>

<%- include partials/foot %>
